<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".mqtt_act">

    <TextView
        android:id="@+id/textView9"
        android:layout_width="260dp"
        android:layout_height="51dp"
        android:text="MQTT BOOKS"
        android:textSize="36sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.548"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.057" />

    <LinearLayout
        android:layout_width="327dp"
        android:layout_height="340dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.55">

        <Button
            android:id="@+id/button8"
            android:layout_width="match_parent"
            android:layout_height="64dp"
            android:onClick="LibroRevival"
            android:text="Revival" />

        <Button
            android:id="@+id/button7"
            android:layout_width="match_parent"
            android:layout_height="62dp"
            android:onClick="LibroFarenheit"
            android:text="Farenheith" />

        <Button
            android:id="@+id/button6"
            android:layout_width="match_parent"
            android:layout_height="64dp"
            android:onClick="Libro1ElAlquimista"
            android:text="El Alquimista" />

        <Button
            android:id="@+id/button3"
            android:layout_width="match_parent"
            android:layout_height="61dp"
            android:onClick="LibroElPoder"
            android:text="El poder" />

        <Button
            android:id="@+id/button2"
            android:layout_width="match_parent"
            android:layout_height="59dp"
            android:onClick="LibroElDespertar"
            android:text="El Despertar" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>